<%= simple_form_for(@reimbursement) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :sn %>
    <%= f.input :reimburse_on %>
    <%= f.input :staff %>
    <%= f.input :status %>
    <%= f.input :organization_id %>
    <%= f.input :abstract %>
    <%= f.input :amount %>
    <%= f.input :expense_id %>
    <%= f.input :invoice_no %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
